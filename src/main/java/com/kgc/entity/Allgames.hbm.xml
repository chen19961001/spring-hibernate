<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.kgc.entity.Allgames" table="allgames" schema="5173game">
        <id name="id">
            <column name="id" sql-type="double" precision="-1"/>
        </id>
        <property name="cname">
            <column name="cname" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="initial">
            <column name="initial" sql-type="varchar(6)" length="6" not-null="true"/>
        </property>
        <property name="isHot">
            <column name="isHot" sql-type="varchar(6)" length="6" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="onOff">
            <column name="onOff" sql-type="varchar(15)" length="15" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>